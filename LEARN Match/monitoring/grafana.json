{
  "title": "AI Lifecycle Monitoring",
  "panels": [
    {
      "type": "graph",
      "title": "Total Requests",
      "targets": [
        {
          "expr": "app_requests_total",
          "legendFormat": "Requests",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Recommendations Served",
      "targets": [
        {
          "expr": "recommendations_served",
          "legendFormat": "Recommendations",
          "refId": "B"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Processing Time",
      "targets": [
        {
          "expr": "processing_time_seconds",
          "legendFormat": "Processing Time (s)",
          "refId": "C"
        }
      ]
    }
  ]
}
